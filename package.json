  },
  "scripts": {
    "clean": "rm -rf ./lib/*",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" \"src/**/*.json\"",
    "start": "npm run watch",
    "test-unit": "ts-node ./node_modules/jasmine/bin/jasmine ./test/unit/index.ts",
    "ts-build": "tsc -p tsconfig.json && npm run copy-dts",
    "ts-lint": "tslint -p tsconfig.json",
    "ts-lint-check": "tslint-config-prettier-check ./tslint.json",
    "ts-rebuild": "npm run clean && npm run ts-build",
    "ts-watch": "tsc-watch -p tsconfig.json --preserveWatchOutput --onSuccess \"npm run ts-lint\"",
    "watch": "npm run clean && npm run ts-watch",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib"
  },
  "typings": "./lib/index",
  "version": "0.1.3"
